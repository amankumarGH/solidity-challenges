pragma solidity 0.8.26;

contract Challenge1 {

  uint public uid;
  bool public abbv;
  string public name;
  address payable public token;

function setNumber(uint _number) public returns(uint) {
    uid = _number;
    return uid;
  }

  function getNumber() public view returns(uint) {
    return uid;
  }

  function setAbbv(bool _address) public returns(bool) {
    abbv = _address;
    return abbv;
  }

  function getAddress() public view returns(bool) {
    return abbv;
  }

  function setName(string memory _name) public returns(string memory) {
    name = _name;
    return name;
  }

  function getName() public view returns(string memory) {
    return name;
  }

function setToken(address payable _token) public returns(address payable) {
    token = _token;
    return token;
  }

  function getToken() public view returns(address payable) {
    return token;
  }
}

